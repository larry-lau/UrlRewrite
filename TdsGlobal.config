<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="3.5" DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
	<PropertyGroup>
		<CompactSitecoreItemsInProjectFile>True</CompactSitecoreItemsInProjectFile>
		<SitecoreAssemblyPath>$(UserProfile)\.nuget\packages</SitecoreAssemblyPath>
	</PropertyGroup>
	<PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
		<SitecoreDeployFolder>C:\inetpub\wwwroot\sc10sc.dev.local</SitecoreDeployFolder>
		<SitecoreWebUrl>https://sc10sc.dev.local</SitecoreWebUrl>
		<RecursiveDeployAction>SitecoreRecycle</RecursiveDeployAction>
		<InstallSitecoreConnector>True</InstallSitecoreConnector>
		<SitecoreAccessGuid>a5e87452-7f95-4d6d-86d6-61c4f7acfccf</SitecoreAccessGuid>
		<DisableFileDeployment>True</DisableFileDeployment>
		<LightningDeployMode>True</LightningDeployMode>
		<GeneratePackage>False</GeneratePackage>
	</PropertyGroup>
	<PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
		<OutputPath>$(SolutionDir)\TdsGeneratedPackages\Release\</OutputPath>
		<GeneratePackage>False</GeneratePackage>
		<SeperateFilesAndItems>False</SeperateFilesAndItems>
		<PackageName>$(MSBuildProjectName)</PackageName>
		<PackageVersion>1.0</PackageVersion>
		<PackageExcludeCode>True</PackageExcludeCode>
		<AddDateTimeToPackageName>False</AddDateTimeToPackageName>
	</PropertyGroup>
</Project>
